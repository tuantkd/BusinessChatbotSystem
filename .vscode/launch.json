{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Django Server",
            "type": "debugpy",
            "request": "launch",
            "module": "manage",
            "args": [
                "runserver"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/DjangoServer",
            "env": {
                "DJANGO_SETTINGS_MODULE": "enterprise_registration_app.settings"
            },
            "python": "${workspaceFolder}/DjangoServer/venv/Scripts/python.exe"
        },
        {
            "name": "Rasa Server",
            "type": "debugpy",
            "request": "launch",
            "module": "rasa",
            "args": [
                "run",
                "--model",
                "models",
                "--endpoints",
                "endpoints.yml",
                "--port",
                "5055",
                "--enable-api",
                "--debug"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/RasaServer",
            "python": "${workspaceFolder}/RasaServer/venv/Scripts/python.exe"
        },
        {
            "name": "Rasa Action Server",
            "type": "debugpy",
            "request": "launch",
            "module": "rasa",
            "args": [
                "run",
                "actions",
                "--port",
                "5005",
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/RasaServer",
            "python": "${workspaceFolder}/RasaServer/venv/Scripts/python.exe"
        },
        {
            "name": "Rasa Train",
            "type": "debugpy",
            "request": "launch",
            "module": "rasa",
            "args": [
                "train",
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/RasaServer",
            "python": "${workspaceFolder}/RasaServer/venv/Scripts/python.exe"
        },
        {
            "name": "Rasa Shell",
            "type": "debugpy",
            "request": "launch",
            "module": "rasa",
            "args": [
                "shell",
                "--port",
                "5056",
                "--debug",
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/RasaServer",
            "python": "${workspaceFolder}/RasaServer/venv/Scripts/python.exe"
        },
        {
            "name": "Rasa Interactive",
            "type": "debugpy",
            "request": "launch",
            "module": "rasa",
            "args": [
                "shell",
                "--port",
                "5057",
                "--debug",
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/RasaServer",
            "python": "${workspaceFolder}/RasaServer/venv/Scripts/python.exe"
        },
        {
            "name": "Rasa Data Validate",
            "type": "debugpy",
            "request": "launch",
            "module": "rasa",
            "args": [
                "data",
                "validate",
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/RasaServer",
            "python": "${workspaceFolder}/RasaServer/venv/Scripts/python.exe"
        },
    ]
}